
// Q 13 Write a program that takes an array of numbers as input and calculates the sum of all the numbers using a for loop.
var inputcount = prompt("Enter how many elements to add in array :");
var lenarray = parseInt(inputcount);
var Arr = [];
var sum = 0;
for (let i = 1; i <= lenarray; i++){
  var input = parseInt(prompt("Enter element :"))
  Arr.push(input);
  sum = sum + input; 
}
console.log(Arr)
console.log("Sum of all elements in array is :" + " " + sum);





